// Test nested tuple pattern matching
// This exercises multi-level occurrence paths

pub fn match_nested_tuple(t: ((bool, bool), bool)) -> u8 {
    match t {
        ((true, true), true) => 7,
        ((true, true), false) => 6,
        ((true, false), _) => 5,
        ((false, _), true) => 2,
        ((false, _), false) => 1,
    }
}

pub fn match_deeply_nested(t: (((bool, bool), bool), bool)) -> u8 {
    match t {
        (((true, true), true), true) => 15,
        (((true, _), _), _) => 8,
        (((false, _), _), _) => 0,
    }
}
