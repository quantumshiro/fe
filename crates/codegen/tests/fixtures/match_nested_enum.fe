
enum Inner {
    Unit,
    Value(u8)
}

enum Outer {
    First(Inner),
    Second(u8)
}

// Test nested enum pattern matching with bindings
pub fn match_nested_enum(outer: Outer) -> u8 {
    match outer {
        Outer::First(Inner::Unit) => 0
        Outer::First(Inner::Value(x)) => x
        Outer::Second(y) => y
    }
}
