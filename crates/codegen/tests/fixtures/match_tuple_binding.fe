// Test tuple pattern matching WITH variable bindings
// This exercises decision tree leaf.bindings for tuple elements

pub fn match_tuple_extract(t: (u8, u8)) -> u8 {
    match t {
        (0, y) => y,
        (x, 0) => x,
        (a, b) => a + b,
    }
}

pub fn match_nested_extract(t: ((u8, u8), u8)) -> u8 {
    match t {
        ((0, 0), z) => z,
        ((x, y), 0) => x + y,
        ((a, _), c) => a + c,
    }
}
