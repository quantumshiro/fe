fn read_x() -> u256 uses (x: u256) {
    x
}

fn bump_x() uses (mut x: u256) {
    x += 1
}

fn test_mem_ptr() {
    let p = core::effect_ref::MemPtr<u256> { addr: 0 }
    with (p) {
        let _v = read_x()
        bump_x()
    }
}

fn test_calldata_ptr_readonly() {
    let c = core::effect_ref::CalldataPtr<u256> { offset: 0 }
    with (c) {
        let _v = read_x()
    }
}

